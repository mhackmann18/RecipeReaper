{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": ["airbnb-base", "prettier"],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "prettier/prettier": "error",
        "no-console": "off",
        "global-require": "off",
        "camelcase": "off",
        "func-names": "off",
        "no-plusplus": "off",
        "no-restricted-syntax": "off"
    }
}
